    "first_char = user_input[0]\n",
    "new_string = \"\"\n",
    "for char in user_input:\n",
    "    if char == first_char:\n",
    "        new_string += str_replace\n",
    "    else:\n",
    "        new_string += char\n",
    "print(new_string)\n"
